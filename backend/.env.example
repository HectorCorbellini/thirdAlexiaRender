DATABASE_URL="postgresql://johndoe:randompassword@localhost:5432/mydb?schema=public"
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"
WHATSAPP_ACCESS_TOKEN="your-meta-access-token"
WHATSAPP_PHONE_NUMBER_ID="your-phone-number-id"
# Messaging Platform Configuration
# Select the messaging platform to use
# Options: whatsapp, telegram
# Default: whatsapp
MESSAGING_PLATFORM=whatsapp

# Telegram Configuration (required when MESSAGING_PLATFORM=telegram)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Map Provider Configuration
# Options: nominatim, google
# Default: nominatim (free)
MAP_PROVIDER=nominatim

# Google Places API (optional, for enhanced location services)
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# AI Provider Configuration
# Select the AI provider to use
# Options: openai, groq
# Default: openai
AI_PROVIDER=openai

# OpenAI Configuration (required when AI_PROVIDER=openai)
OPENAI_API_KEY="your-openai-api-key"
OPENAI_MODEL="gpt-3.5-turbo"

# Groq Configuration (required when AI_PROVIDER=groq)
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=llama3-8b-8192
PORT=3001
NODE_ENV="development"
